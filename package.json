{
  "name": "lindas-cube-link",
  "version": "0.2.4",
  "private": true,
  "description": "Cube Schema - LINDAS Fork",
  "type": "module",
  "scripts": {
    "build": "./build.sh",
    "trifid": "lindas-trifid --verbose --config=trifid/config.json",
    "trifid:local": "lindas-trifid --verbose --config=trifid/config.local.json",
    "lint": "standard",
    "test": "run-p --aggregate-output -cns \"test:* -- {@}\" --",
    "test:observations": "node ./test/validate-shacl.js",
    "test:basic-cube-constraint": "node ./test/validate-metadata.js basic-cube-constraint",
    "test:standalone-cube-constraint": "node ./test/validate-metadata.js standalone-cube-constraint",
    "test:standalone-constraint-constraint": "node ./test/validate-metadata.js standalone-constraint-constraint",
    "test:profile-visualize": "node ./test/validate-metadata.js profile-visualize",
    "test:profile-opendataswiss": "node ./test/validate-metadata.js profile-opendataswiss",
    "test:profile-opendataswiss-lindas": "node ./test/validate-metadata.js profile-opendataswiss-lindas",
    "release": "changeset tag"
  },
  "files": [
    "validate.js",
    "validation",
    "bin",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/SwissFederalArchives/lindas-cube-link.git"
  },
  "keywords": [
    "cube",
    "rdf",
    "schema",
    "lindas"
  ],
  "author": "Thomas Bergwinkl <bergi@axolotlfarm.org> (https://www.bergnet.org/people/bergi/card#me)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SwissFederalArchives/lindas-cube-link/issues"
  },
  "homepage": "https://github.com/SwissFederalArchives/lindas-cube-link",
  "dependencies": {
    "@lindas/trifid": "^6.0.1",
    "node-fetch-cache": "^3.1.3"
  },
  "overrides": {
    "@types/node": "^22.0.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.0",
    "@lindas/barnard59": "^6.0.8",
    "@lindas/barnard59-base": "^3.0.2",
    "@lindas/barnard59-cube": "^2.0.3",
    "@lindas/barnard59-formats": "^5.0.2",
    "@lindas/barnard59-http": "^3.0.2",
    "@lindas/barnard59-sparql": "^3.0.2",
    "@lindas/shacl-test": "^0.1.7",
    "approvals": "^7.3.0",
    "n3": "^1.17.2",
    "npm-run-all": "^4.1.5",
    "pm2": "^6.0.14",
    "rdf-ext": "^2.5.1",
    "rdf-validate-shacl": "^0.6.5",
    "respec": "^35.0.0",
    "serve": "^14.2.4",
    "standard": "^17.1.0"
  }
}
