{
  "baseConfig": "cwd:trifid/config-base.json",
  "handler": {
    "root": {
      "module": "lindas-trifid-handler-fetch",
      "options": {
        "resource": "https://cube.link/",
        "url": "cwd:vocab.ttl",
        "contentType": "text/turtle",
        "cache": "true"
      }
    },
    "meta": {
      "path": "/meta/",
      "module": "lindas-trifid-handler-fetch",
      "options": {
        "resource": "https://cube.link/meta/",
        "url": "cwd:meta/meta.ttl",
        "contentType": "text/turtle",
        "cache": "true"
      }
    },
    "relation": {
      "path": "/relation/",
      "module": "lindas-trifid-handler-fetch",
      "options": {
        "resource": "https://cube.link/relation/",
        "url": "cwd:relation/relation.ttl",
        "contentType": "text/turtle",
        "cache": "true"
      }
    }
  },
  "renderer": {
    "root": null,
    "meta": {
      "path": "/meta/",
      "module": "lindas-trifid-entity-renderer",
      "template": "cwd:dist/meta.html",
      "templateError": "trifid:views/error.html",
      "alternativeMediaTypes": [
        "application/ld+json",
        "application/n-triples",
        "application/rdf+xml",
        "text/turtle"
      ]
    },
    "relation": {
      "path": "/relation/",
      "module": "lindas-trifid-entity-renderer",
      "template": "cwd:dist/relation.html",
      "templateError": "trifid:views/error.html",
      "alternativeMediaTypes": [
        "application/ld+json",
        "application/n-triples",
        "application/rdf+xml",
        "text/turtle"
      ]
    },
    "cube": {
      "module": "lindas-trifid-entity-renderer",
      "template": "cwd:dist/cube.html",
      "templateError": "trifid:views/error.html",
      "alternativeMediaTypes": [
        "application/ld+json",
        "application/n-triples",
        "application/rdf+xml",
        "text/turtle"
      ]
    }
  },
  "staticViews": {
    "/": null
  },
  "staticFiles": {
    "images": {
      "path": "/img/",
      "folder": "cwd:img"
    }
  },
  "plugins": {
    "vocab-redirect": {
      "priority": 55,
      "module": "lindas-trifid-core:./plugins/middleware",
      "middleware": "cwd:trifid/redirect.cjs"
    }
  }
}
